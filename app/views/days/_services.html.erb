<% date ||= params[:date] %>

<%= link_to image_tag('wolfram.ico', style: 'height: 20px;'), "http://www.wolframalpha.com/input/?i=#{date}" %>
<%= link_to image_tag('gmaps.png', style: 'height: 20px;'), "https://www.google.com/maps/timeline?pb=!1m2!1m1!1s#{date}" %>
<%= link_to image_tag('gphotos.ico', style: 'height: 20px;'), "https://photos.google.com/search/#{date}" %>
<%= link_to image_tag('amazon.ico', style: 'height: 20px;'), ("https://www.amazon.com/photos/family?lcf=time&timeYear=%s&timeMonth=%s" % date.split('-')) %>
